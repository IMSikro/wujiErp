{
	"$schema": "https://gitee.com/dotnetchina/Furion/raw/net6/schemas/v3/furion-schema.json",
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"SpecificationDocumentSettings": {
		"DocumentTitle": "无忌ERP",
		"RoutePrefix": "wujierp",
		"DefaultGroupName": "wuji",
		"RouteTemplate": "wujierp/{documentName}.json",
		"GroupOpenApiInfos": [
			{
				"Group": "wuji",
				"Title": "无忌ERP",
				"Description": "地振高冈，一派溪山千古秀。<br />门朝大海，三河合水万年流。",
				"Version": "v2",
				"License": {
					"Name": "© 2021 JinDex"
				}
			}
		]
	},
	"AppSettings": {
		"InjectMiniProfiler": true
	},
	"DynamicApiControllerSettings": {
		"SupportedMvcController": true,
		"DefaultHttpMethod": "POST",
		"LowercaseRoute": false,
		"CamelCaseSeparator": "",
		"UrlParameterization": true,
		"AbandonControllerAffixes": [
			"Controller"
		],
		"AbandonActionAffixes": [
			"Async"
		],
		"KeepVerb": true,
		"KeepName": true
	}
}